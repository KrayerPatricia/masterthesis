ActCropsGrass_OtherChar(Regions,Activities,"RED: tot. IrrigW per t MOutput1 (m3), account. water scarc.","AllProdSyst","AllProdCond")
         = sum((Countries_Pfister2011,Crops_Pfister2011,CountryCodes_Pfister2011)$(MatchCountriesPfister2011_SOLm(Countries_Pfister2011,Regions) AND MatchCropsPfister2011_SOLm(Crops_Pfister2011,Activities)),
                 RED_water_total(Crops_Pfister2011,Countries_Pfister2011,CountryCodes_Pfister2011));
ActCropsGrass_OtherChar(Regions,Activities,"RED: exp. IrrigW per t MOutput1 (m3), account. water scarc.","AllProdSyst","AllProdCond")
         = sum((Countries_Pfister2011,Crops_Pfister2011,CountryCodes_Pfister2011)$(MatchCountriesPfister2011_SOLm(Countries_Pfister2011,Regions) AND MatchCropsPfister2011_SOLm(Crops_Pfister2011,Activities)),
                 RED_water(Crops_Pfister2011,Countries_Pfister2011,CountryCodes_Pfister2011));
ActCropsGrass_OtherChar(Regions,Activities,"RED: min. IrrigW per t MOutput1 (m3), account. water scarc.","AllProdSyst","AllProdCond")
         = sum((Countries_Pfister2011,Crops_Pfister2011,CountryCodes_Pfister2011)$(MatchCountriesPfister2011_SOLm(Countries_Pfister2011,Regions) AND MatchCropsPfister2011_SOLm(Crops_Pfister2011,Activities)),
                 RED_water_deficit(Crops_Pfister2011,Countries_Pfister2011,CountryCodes_Pfister2011));
ActCropsGrass_OtherChar(Regions,Activities,"IrrigW per t MOutput1 (m3) - total demand","AllProdSyst","AllProdCond")
