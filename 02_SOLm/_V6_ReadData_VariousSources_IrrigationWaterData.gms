         = sum((Countries_Pfister2011,Crops_Pfister2011,CountryCodes_Pfister2011)$(MatchCountriesPfister2011_SOLm(Countries_Pfister2011,Regions) AND MatchCropsPfister2011_SOLm(Crops_Pfister2011,Activities)),
                 BW_deficit(Crops_Pfister2011,Countries_Pfister2011,CountryCodes_Pfister2011));
ActCropsGrass_OtherChar(Regions,Activities,"IrrigW per t MOutput1 (m3) - exp. demand: mean of real,total","AllProdSyst","AllProdCond")
         = sum((Countries_Pfister2011,Crops_Pfister2011,CountryCodes_Pfister2011)$(MatchCountriesPfister2011_SOLm(Countries_Pfister2011,Regions) AND MatchCropsPfister2011_SOLm(Crops_Pfister2011,Activities)),
                 BW_expected(Crops_Pfister2011,Countries_Pfister2011,CountryCodes_Pfister2011));

ActCropsGrass_OtherChar(Regions,Activities,"Share irrigated in total area (share ha)","AllProdSyst","AllProdCond")
         = sum((Countries_Pfister2011,Crops_Pfister2011,CountryCodes_Pfister2011)$(MatchCountriesPfister2011_SOLm(Countries_Pfister2011,Regions) AND MatchCropsPfister2011_SOLm(Crops_Pfister2011,Activities)),
                 IrrShares(Crops_Pfister2011,Countries_Pfister2011,CountryCodes_Pfister2011));
ActCropsGrass_OtherChar(Regions,Activities,"Water stress index (ratio): lack of water for donwstream use","AllProdSyst","AllProdCond")
         = sum((Countries_Pfister2011,Crops_Pfister2011,CountryCodes_Pfister2011)$(MatchCountriesPfister2011_SOLm(Countries_Pfister2011,Regions) AND MatchCropsPfister2011_SOLm(Crops_Pfister2011,Activities)),
                 WSI(Crops_Pfister2011,Countries_Pfister2011,CountryCodes_Pfister2011));

*thus, we have all these different water use indicators available;
*the most relevant of those are the total irrigation water demand, that is, given the crop characteristics and natural conditions, how much irrigation water is needed to grow the crop - irrespective of irrigation infrastructure being available or not, etc., i.e.
*         ActCropsGrass_OtherChar(Regions,Activities,"IrrigW per t MOutput1 (m3) - total demand","AllProdSyst","AllProdCond")
*and the scarcity corrected value for this, assessing this irrigation need in the context of water stress, i.e.
*         ActCropsGrass_OtherChar(Regions,Activities,"RED: tot. IrrigW per t MOutput1 (m3), account. water scarc.","AllProdSyst","AllProdCond")
*from these, we will build per ha values here - and in the scenarios, after having assigned scenario yield values:

ActCropsGrass_Inputs(Regions,Activities,"Irrigation water (m3)","AllProdSyst","AllProdCond")
         = ActCropsGrass_OtherChar(Regions,Activities,"IrrigW per t MOutput1 (m3) - total demand","AllProdSyst","AllProdCond")
                 *ActCropsGrass_Outputs(Regions,Activities,"MainOutput1 (t)","AllProdSyst","AllProdCond");

ActCropsGrass_Inputs(Regions,Activities,"Irrigation water (m3) - water stress adjusted","AllProdSyst","AllProdCond")
         = ActCropsGrass_OtherChar(Regions,Activities,"RED: tot. IrrigW per t MOutput1 (m3), account. water scarc.","AllProdSyst","AllProdCond")
                 *ActCropsGrass_Outputs(Regions,Activities,"MainOutput1 (t)","AllProdSyst","AllProdCond");


